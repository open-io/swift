{
    "files": [
        {
            "aql": {
                "items.find": {
                    "repo": "pu-objectstorage-swift-static",
                    "$and": [
                        {
                            "path": {"$match": "*coverage/dev"}
                        },
                        {
                            "type": "folder"
                        },
                        {
                            "created": {"$lt": "${ddate}"}
                        }
                    ]
                }
            }
        },
        {
            "aql": {
                "items.find": {
                    "repo": "pu-objectstorage-swift-static",
                    "$and": [
                        {
                            "path": {"$match": "*coverage/master"}
                        },
                        {
                            "type": "folder"
                        },
                        {
                            "created": {"$lt": "${mdate}"}
                        }
                    ]
                }
            }
        }
    ]
}
